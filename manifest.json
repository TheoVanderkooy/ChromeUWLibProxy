{
    "manifest_version":2,
    "name": "UWLibProxy",
    "version": "1.0",

    "description": "Opens the current page through tue UWaterloo library proxy",
    "author": "Theo Vanderkooy",

    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
